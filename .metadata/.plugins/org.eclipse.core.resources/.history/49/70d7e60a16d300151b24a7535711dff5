package button;
// Code skeleton for the OpCom class in the Buttons exercise

public class OpCom extends Thread {
	private Regul regul;
	private double K;

	// Constructor
	public OpCom(Regul regul, int priority) {
		this.regul = regul;
		setPriority(priority);
	}

private synchronized setK(double K){
	this.K=K;
}

	// run method
	public void run();
}
